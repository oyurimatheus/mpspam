/*
Estilo padrão do QuickTip
*/

div.qtp_message 
{
	background-color: #f3f3f3; 
	border-top: 1px solid #999; 
	text-align: center; 
	color: #333;
	padding:5px;
	
}
div.qtp_message div,
div.qtp_message div.qtp_warning
{
	clear:both; 
	color:#0b55c4; 
	background-color:#FFFACD;
	text-align:center;
	white-space: nowrap;
	font-size: 12px;
	padding: 3px;
	font-family: Arial;
}