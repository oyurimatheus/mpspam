var SearchProxy={url:"api",buscar:function(r,e){return util.initLoader(),AjaxPool.abortAll(),$.ajax({type:"GET",url:SearchProxy.url+"/search",data:{q:r,srv:JSON.stringify(e)},complete:function(){util.hideLoader(),$("#salvar").prop("disabled",!1)},beforeSend:function(){$("#salvar").prop("disabled",!0)}})},getVersao:function(){return $.ajax({url:SearchProxy.url+"/versao",type:"GET",contentType:"application/json"})},getPerfis:function(){return $.ajax({url:SearchProxy.url+"/perfis",type:"GET",contentType:"application/json"})},getAgrupamentos:function(){return $.ajax({url:SearchProxy.url+"/agrupamentos",type:"POST"})},visualizarNotificacao:function(){return $.ajax({url:SearchProxy.url+"/notificacoes",beforeSend:function(r){r.setRequestHeader("Authorization","token "+Funcoes.getCookie("iPlanetDirectoryPro"))},type:"POST"})}};