function checkaTodos(form,pai,elemento) {
  if(document.forms[form].elements[pai].checked!=false){
    for (var i=0;i<document.forms[form].elements.length;i++)
    {
      var x = document.forms[form].elements[i];
      if (x.name == elemento)
      x.checked = false;
    }
  }
  else{
    for (var i=0;i<document.forms[form].elements.length;i++)
    {
      var x = document.forms[form].elements[i];
      if (x.name == elemento)
      x.checked = true;
    }
  }
}

