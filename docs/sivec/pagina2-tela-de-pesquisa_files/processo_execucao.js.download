		$("<div class=\"textotab2 text-center\" title=\"Processo Execução !\"><br>Informe o Tipo Local de Prisão para Inclusão!</div>").dialog({
	        width: 450,
	        modal: true,
	        buttons: {
		    	"Ok": function() {
		    	$(this).dialog("close");
				}
	 		}
	   	 }).addClass('ui-state-error ui-corner-all');
		return false;
	}
	if ($("#idcodEstadoLocal").val() == "" && $("#idTipoLocal").val() != "") {
		$('#idcodEstadoLocal').addClass('ui-state-error');
		$("<div class=\"textotab2 text-center\" title=\"Processo Execução !\"><br>Informe o Estado de Prisão para Inclusão!</div>").dialog({
	        width: 450,
	        modal: true,
	        buttons: {
		    	"Ok": function() {
		    	$(this).dialog("close");
				}
	 		}
	   	 }).addClass('ui-state-error ui-corner-all');
		return false;
	}
	if ($("#idcodEstadoLocal").val() == 27000 && $("#idcodCidadeLocal").val() == "") {
		$('#idcodCidadeLocal').addClass('ui-state-error');
		$("<div class=\"textotab2 text-center\" title=\"Processo Execução !\"><br>Informe o Cidade de Prisão para Inclusão!</div>").dialog({
	        width: 450,
	        modal: true,
	        buttons: {
		    	"Ok": function() {
		    	$(this).dialog("close");
		    	$('#idcodCidadeLocal').removeClass('ui-state-error');
				}
	 		}
	   	 }).addClass('ui-state-error ui-corner-all');
		return false;
	}
	if ($("#idCodLocal").val() == "" && $("#idTipoLocal").val() != "" && $("#idcodEstadoLocal").val() != "") {
		$('#idCodLocal').addClass('ui-state-error');
		$("<div class=\"textotab2 text-center\" title=\"Processo Execução !\"><br>Informe o Local de Prisão para Inclusão!</div>").dialog({
	        width: 450,
	        modal: true,
	        buttons: {
		    	"Ok": function() {
		    	$(this).dialog("close");
		    	$('#idCodLocal').removeClass('ui-state-error');
				}
	 		}
	   	 }).addClass('ui-state-error ui-corner-all');
		return false;
	}
	else {
		if ($("#numProcExec").val() != "" && $("#idTipoLocal").val() != "") {
			var elemen = $('#' + $("#numProcExec").val());
			
			if (elemen.val() != undefined && elemen.val() != null) {
				$( "<div>O Número de processo " + $("#numProcExec").val() + " já esta cadastrado neste lote!</div>" ).dialog({
					width: 450,  modal: true, buttons: {
						Ok: function() {
							$( this ).dialog( "close" );
						}
					}
				    });	
				return false;
			}
			document.forms[0].action = "processo_execucao_insere_lote_prisao.do";
			document.forms[0].submit();
		} else {
			$('#numProcExec').addClass('ui-state-error');
			$("<div class=\"textotab2 text-center\" title=\"Processo Execução !\"><br>Informe o número do Processo de Execução!</div>").dialog({
		        width: 450,
		        modal: true,
		        buttons: {
			    	"Ok": function() {
			    	$(this).dialog("close");
			    	$('#numProcExec').removeClass('ui-state-error');
					}
		 		}
		   	 }).addClass('ui-state-error ui-corner-all');
			return false;
		}
	}
}

