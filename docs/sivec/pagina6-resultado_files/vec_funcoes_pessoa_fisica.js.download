function pesquisa_por_nome() {
	if($('#idNomePesq').val() == "") {
		$('#idNomePesq').addClass('ui-state-error');
		$("<div class=\"textotab2 text-center\" title=\"Pesquisa por Réu!\"><br>Favor informar o nome para efetuar a pesquisa!  </div>").dialog({
			width: 450, modal: true, buttons: {
				"Ok": function() {
					$(this).dialog("close");
				}
			}
		}).addClass('ui-state-error ui-corner-all');
		return false;
	}
	else {
		desabilitar('procura');
		document.forms[0].action = "pesq_pf_fonetica.do";
		document.forms[0].submit();
	}
}

function pesquisa_por_opcoes() {
	var nome = $('#idNomePesq').val();
	var pai = $("#pai").val();
	var mae = $("#mae").val();
	var alcunha = $('#alcunha').val();
	if(nome == "" && pai == "" && mae == "" && alcunha == "") {
		$('#idNomePesq').addClass('ui-state-error');
		$('#pai').addClass('ui-state-error');
		$('#mae').addClass('ui-state-error');
		$('#alcunha').addClass('ui-state-error');
		
		$("<div class=\"textotab2 text-center\" title=\"Pesquisa por Réu!\"><br>Favor informar os campos obrigatórios para efetuar a pesquisa!</div>").dialog({
			width: 450, modal: true, buttons: {
				"Ok": function() {
					$(this).dialog("close");
				}
			}
		}).addClass('ui-state-error ui-corner-all');
		return false;
	}
	else {
		desabilitar('procura');
		document.forms[0].action = "pesq_pf_fonetica.do";
		document.forms[0].submit();
	}
}